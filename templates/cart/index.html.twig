{% extends 'base.html.twig' %}
{% block title %}
    Panier
{% endblock %}
{% block body %}

    <h1>Panier</h1>
    <ul class="cartLines">
        {% for cartLine in cartLines %}
            <li><span class="product">{{ cartLine.product }}</span>, <span class="quantity">{{ cartLine.quantity }}</span></li>
        {% endfor %}
    </ul>
{% endblock %}